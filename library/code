template<class A>
pair<A,A> euclid(A a,A b,A &d){
    if(a==0)return {0,d/b};
    else if(b==0)return {d/a,0};
    else{
        if(a>=b){
            A x=a/b;
            a=a%b;
            pair<A,A> res=euclid(a,b,d);
            return {res.first,res.second-x*res.first};
        }else{
            A x=b/a;
            b=b%a;
            pair<A,A> res=euclid(a,b,d);
            return {res.first-x*res.second,res.second};
        }
    }
}